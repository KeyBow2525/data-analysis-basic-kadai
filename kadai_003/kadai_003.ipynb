    "# numpyをimport。\n",
    "import numpy as np\n",
    "\n",
    "# 行列AとBを作成。\n",
    "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
    "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
    "\n",
    "# AとBの積を求めて出力。\n",
    "C = np.dot(A, B)\n",
    "print(C)\n",
    "\n",
    "# Cの要素の最大値を求める。\n",
    "C.max()"
   ]
  }
